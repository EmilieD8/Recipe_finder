{
    "name": "vegan_food_searcher",
    "version": "1.0.0",
    "scripts": {
        "build:backend": "cd backend && tsc",
        "start:backend": "cd backend && npm run build && node dist/server.js",
        "dev:backend": "cd backend && ts-node-dev --respawn src/server.ts",
        "start:frontend": "cd frontend && npm start",
        "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\""
    },
    "dependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.28.0"
    },
    "devDependencies": {
        "@babel/preset-react": "^7.25.9",
        "@babel/preset-typescript": "^7.26.0",
        "@types/node": "^22.9.0",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@types/react-router-dom": "^5.3.3",
        "concurrently": "^9.1.0",
        "sass": "^1.81.0",
        "typescript": "^5.6.3"
    }
}
